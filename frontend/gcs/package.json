{
  "name": "lock2a-gcs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"GCS,RecordingAnalysis\" --prefix-colors \"blue,green\" \"npm run dev:gcs-only\" \"npm run start:recording-analysis\"",
    "dev:gcs-only": "next dev --turbopack -p 8765",
    "start:recording-analysis": "cd ../../RecordingAnalysis/frontend && npm run dev -- -p 9876",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "next": "^15.5.9",
    "react": "19.1.0",
    "react-circular-progressbar": "^2.2.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
